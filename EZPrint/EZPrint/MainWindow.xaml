<Window x:Class="EZPrint.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EZPrint" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="23" Margin="10,38,0,0" TextWrapping="Wrap" Text="15someguy" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblDomain" Content="@ridgewood.k12.nj.us" HorizontalAlignment="Left" Margin="135,35,0,0" VerticalAlignment="Top" Width="134"/>
        <PasswordBox x:Name="pwdPassword" HorizontalAlignment="Left" Margin="10,66,0,0" VerticalAlignment="Top" Width="247"/>
        <Button x:Name="btnLoadPrinters" Content="Load Printers" HorizontalAlignment="Left" Margin="94,89,0,0" VerticalAlignment="Top" Width="75" Click="btnLoadPrinters_Click"/>
        <ListBox x:Name="lstPrinters" HorizontalAlignment="Left" Height="100" Margin="311,35,0,0" VerticalAlignment="Top" Width="180" SelectionChanged="lstPrinters_SelectionChanged_1"/>
        <Button x:Name="btnLoadFile" Content="Load File" HorizontalAlignment="Left" Margin="311,140,0,0" VerticalAlignment="Top" Width="180" Click="btnLoadFile_Click" Height="49"/>
        <Button x:Name="btnPrint" Content="Print" HorizontalAlignment="Left" Margin="311,194,0,0" VerticalAlignment="Top" Width="180" Click="btnPrint_Click" Height="49"/>
    </Grid>
</Window>
